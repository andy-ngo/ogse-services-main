                "colors": ["rgba(242,240,247,0.8)", "rgba(218,218,235,0.8)", "rgba(188,189,220,0.8)", "rgba(158,154,200,0.8)", "rgba(117,107,177,0.8)", "rgba(84,39,143,0.8)"]
            },
            "stroke": {
                "type": "static",
                "width": 1,
                "color": "rgba(0,0,0,1)"
            }
        }, {
            "name": "Active (icon scale)",
            "layer": "Hospitals",
            "scale": {
                "type": "quantile",
                "property": "active",
				"buckets": [369, 470, 524, 587, 588, 613],
				"scale": [0.517, 0.633, 0.750, 0.867, 0.983, 1.100],
                "min": 0.4,
                "max": 1.1,
                "classes": 6
            },
            "src": "../app-framework/components/style/icons/hospital.png"
        }
    ],
    "files": [{
		"name": "log.csv",
		"url": "./sample/gis with files/log.csv"
	},{
		"name": "metadata.json",
		"url": "./sample/gis with files/metadata.json"
	},{
		"name": "scenario.json",
		"url": "./sample/gis with files/scenario.json"
	},{
		"name": "simulation_area.geojson",
		"url": "./sample/gis with files/simulation_area.geojson"
	},{
		"name": "hospitals.geojson",
		"url": "./sample/gis with files/hospitals.geojson"
	},{
		"name": "emergency_areas.geojson",
